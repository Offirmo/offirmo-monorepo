{
	"name": "@monorepo-private/ohateoas-browser--react",
	"description": "Web implementation of a Offirmo’s Hypermedia Architecture browser in React",
	"version": "0.0.1",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": "./module/src/index.tsx"
	},
	"source": "module/src/index.tsx",
	"peerDependencies": {
		"react": "^19",
		"tslib": "^2"
	},
	"dependencies": {
		"@monorepo-private/ohateoas": "*",
		"@monorepo-private/rich-text-format": "*",
		"@monorepo-private/rich-text-format--to-react": "*",
		"@monorepo-private/ts--types--web": "*",
		"tiny-invariant": "^1"
	},
	"scripts": {
		"_clean--pkg": "monorepo-script--clean-package …cache …dist",
		"clean": "npm-run-all _clean--pkg",
		"check--ts": "echo \"@monorepo-private/ohateoas-browser--react\" && tsc --noEmit",
		"check--ts--watch": "tsc --noEmit --watch",
		"dev": "run-s clean check--ts--watch",
		"check": "run-s check--ts",
		"_start:parcel:storypad": "parcel serve module/src/__fixtures/storypad/index.html --port 1981 --lazy --no-autoinstall",
		"stories": "npm-run-all clean --parallel _start:parcel:storypad"
	},
	"devDependencies": {
		"@monorepo-private/monorepo-scripts": "*",
		"@monorepo-private/storypad": "*",
		"@monorepo-private/toolbox--parcel": "*",
		"@monorepo-private/ts--types": "*",
		"@offirmo/unit-test-toolbox": "^9",
		"@types/react": "^19",
		"npm-run-all": "^4",
		"react": "^19",
		"react-dom": "^19",
		"tslib": "^2",
		"typescript": "^5"
	}
}
