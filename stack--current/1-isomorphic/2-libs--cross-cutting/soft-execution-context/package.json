{
	"name": "@monorepo-private/soft-execution-context",
	"description": "graph of execution context to do amazing things!",
	"version": "0.0.2",
	"private": true,
	"sideEffects": true,
	"type": "module",
	"exports": {
		".": "./module/src/index.ts"
	},
	"source": "module/src/index.ts",
	"peerDependencies": {
		"tslib": "^2"
	},
	"dependencies": {
		"@monorepo-private/timestamps": "*",
		"@offirmo/error-utils": "^0.0.1",
		"emittery": "^1",
		"tiny-invariant": "^1"
	},
	"scripts": {
		"tests": "node --experimental-strip-types ./node_modules/.bin/mocha -- --bail --config ./node_modules/@offirmo/unit-test-toolbox/module/mocharc.json ./node_modules/@offirmo/unit-test-toolbox/module/mocha-chai-init-node.mjs './module/src/**/*.tests.ts'",
		"check--ts": "echo \"@monorepo-private/soft-execution-context\" && tsc --noEmit",
		"check--ts--watch": "tsc --noEmit --watch",
		"dev": "run-s check--ts--watch",
		"check": "run-s tests check--ts",
		"demo": "node --experimental-strip-types ./module/##doc/demo.ts"
	},
	"devDependencies": {
		"@monorepo-private/monorepo-scripts": "*",
		"@monorepo-private/print-error-to-terminal": "*",
		"@offirmo/practical-logger-core": "^3",
		"@offirmo/unit-test-toolbox": "^9",
		"npm-run-all": "^4",
		"tslib": "^2",
		"typescript": "^5"
	}
}
