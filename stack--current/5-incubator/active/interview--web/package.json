		"react": "^18",
		"react-dom": "^18",
		"tiny-invariant": "^1",
		"tslib": "^2"
	},
	"devDependencies": {
		"@offirmo-private/generator--website-entry-points": "*",
		"@offirmo-private/monorepo-scripts": "*",
		"@offirmo-private/toolbox--parcel": "*",
		"@offirmo/unit-test-toolbox": "^9",
		"npm-run-all": "^4"
	}
