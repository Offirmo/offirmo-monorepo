{
	"name": "@offirmo-private/ohateoas-browser--react",
	"description": "Web implementation of a Offirmo’s Hypermedia Architecture browser in React",
	"version": "0.0.1",
	"author": "Offirmo <offirmo.net@gmail.com> (https://www.offirmo.net/)",
	"license": "Unlicense",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": "./module/index.tsx"
	},
	"source": "module/index.tsx",
	"peerDependencies": {
		"react": "^19",
		"tslib": "^2"
	},
	"dependencies": {
		"@offirmo-private/ohateoas": "*",
		"@offirmo-private/rich-text-format--to-react": "*",
		"tiny-invariant": "^1"
	},
	"scripts": {
		"_clean--pkg": "monorepo-script--clean-package …cache …dist",
		"clean": "npm-run-all _clean--pkg",
		"check--ts": "echo \"@offirmo-private/ohateoas-browser--react\" && tsc --noEmit",
		"check--ts--watch": "tsc --noEmit --watch",
		"dev": "run-s clean check--ts--watch",
		"check": "run-s check--ts",
		"_start:parcel:storypad": "parcel serve module/src/__fixtures/storypad/index.html --port 1981 --lazy --no-autoinstall",
		"stories": "npm-run-all clean --parallel _start:parcel:storypad"
	},
	"devDependencies": {
		"@offirmo-private/monorepo-scripts": "*",
		"@offirmo-private/rich-text-format": "*",
		"@offirmo-private/storypad": "*",
		"@offirmo-private/toolbox--parcel": "*",
		"@offirmo-private/ts-types-web": "*",
		"@offirmo/unit-test-toolbox": "^9",
		"@types/react": "^19",
		"npm-run-all": "^4",
		"react": "^19",
		"react-dom": "^19",
		"tslib": "^2",
		"typescript": "^5"
	}
}
