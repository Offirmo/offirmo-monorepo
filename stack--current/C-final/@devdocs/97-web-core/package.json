{
	"name": "@devdocs/web-core",
	"version": "0.0.1",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": "./module/src/index.ts"
	},
	"source": "module/src/index.ts",
	"dependencies": {
		"@devdocs/db": "*",
		"@devdocs/marketing": "*",
		"@devdocs/state": "*",
		"@monorepo-private/browser-features-detection": "*",
		"@monorepo-private/css--framework": "*",
		"@monorepo-private/page-loaded": "*",
		"@monorepo-private/react--error-boundary": "*",
		"@monorepo-private/soft-execution-context": "*",
		"@monorepo-private/soft-execution-context--browser": "*",
		"@monorepo-private/utils--async": "*",
		"@offirmo/universal-debug-api-browser": "^1",
		"agentation": "^2",
		"github-slugger": "^2",
		"limit-once": "^0.16",
		"markdown-to-jsx": "^9",
		"react": "^19",
		"react-dom": "^19",
		"react-markdown": "^10",
		"react-router": "^7",
		"rehype-highlight": "^7",
		"rehype-slug": "^6",
		"remark-gfm": "^4",
		"tiny-invariant": "^1",
		"tslib": "^2"
	},
	"scripts": {
		"_clean--pkg": "monorepo-script--clean-package …cache …dist",
		"clean": "npm-run-all _clean--pkg",
		"check--ts": "echo \"@devdocs/web-core\" && tsc --noEmit",
		"check--ts--watch": "tsc --noEmit --watch",
		"dev": "run-s clean check--ts--watch",
		"_check": "run-s check--ts",
		"_start:parcel:storypad": "parcel serve module/__fixtures/storypad/index.html --port 1981 --lazy --no-autoinstall",
		"stories": "npm-run-all clean --parallel _start:parcel:storypad",
		"_start:parcel:sandbox": "parcel serve module/__sandbox/index.html --port 1981 --lazy --no-autoinstall",
		"sandbox": "npm-run-all clean --parallel _start:parcel:sandbox"
	},
	"devDependencies": {
		"@devdocs/types": "*",
		"@monorepo-private/monorepo-scripts": "*",
		"@monorepo-private/storypad": "*",
		"@monorepo-private/toolbox--parcel": "*",
		"@offirmo/unit-test-toolbox": "^9",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"npm-run-all": "^4",
		"react": "^19",
		"typescript": "^5"
	}
}
