{
	"name": "@tbrpg/l4-ohateoas",
	"version": "0.0.1",
	"author": "Offirmo <offirmo.net@gmail.com> (https://www.offirmo.net/)",
	"license": "Unlicense",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": "./module/__fixtures/storypad/index.html"
	},
	"source": "module/__fixtures/storypad/index.html",
	"dependencies": {
		"@offirmo-private/ohateoas": "*",
		"@offirmo-private/rich-text-format": "*",
		"@offirmo-private/timestamps": "*",
		"@offirmo-private/ts-types-web": "*",
		"@tbrpg/interfaces": "*",
		"@tbrpg/state": "*",
		"@tbrpg/ui--rich-text": "*",
		"tiny-invariant": "^1",
		"tslib": "^2"
	},
	"scripts": {
		"_clean--pkg": "monorepo-script--clean-package …cache …dist",
		"clean": "npm-run-all _clean--pkg",
		"tests": "node --experimental-strip-types ./node_modules/.bin/mocha -- --bail --config ./node_modules/@offirmo/unit-test-toolbox/module/mocharc.json ./node_modules/@offirmo/unit-test-toolbox/module/mocha-chai-init-node.mjs './module/__fixtures/storypad/**/*.tests.ts'",
		"check--ts": "echo \"@tbrpg/l4-ohateoas\" && tsc --noEmit",
		"check--ts--watch": "tsc --noEmit --watch",
		"dev": "run-s clean check--ts--watch",
		"check": "run-s tests check--ts",
		"_start:parcel:storypad": "parcel serve module/__fixtures/storypad/index.html --port 1981 --lazy --no-autoinstall",
		"stories": "npm-run-all clean --parallel _start:parcel:storypad",
		"_start:parcel:main": "parcel serve module/__fixtures/storypad/index.html --port 1981 --lazy --no-autoinstall",
		"start": "npm-run-all clean --parallel _start:parcel:main"
	},
	"devDependencies": {
		"@offirmo-private/monorepo-scripts": "*",
		"@offirmo-private/ohateoas-browser--react": "*",
		"@offirmo-private/storypad": "*",
		"@offirmo-private/toolbox--parcel": "*",
		"@offirmo/unit-test-toolbox": "^9",
		"npm-run-all": "^4",
		"typescript": "^5"
	}
}
