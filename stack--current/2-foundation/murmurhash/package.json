{
	"name": "@offirmo-private/murmurhash",
	"description": "A reliable, typed and clearly exported murmurHash implementation",
	"version": "0.0.1",
	"author": "Offirmo <offirmo.net@gmail.com>",
	"license": "Unlicense",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"import": "./module/index.ts"
		}
	},
	"source": "module/index.ts",
	"peerDependencies": {
		"tslib": "^2"
	},
	"dependencies": {
		"@offirmo-private/json-stable-stringify": "*",
		"murmurhash3js-revisited": "^3",
		"tiny-invariant": "^1"
	},
	"scripts": {
		"test--unit": "node --experimental-strip-types ./node_modules/.bin/mocha -- --bail --config ./node_modules/@offirmo/unit-test-toolbox/mocharc.json ./node_modules/@offirmo/unit-test-toolbox/mocha-chai-init-node.mjs './module/**/*.tests.ts'",
		"test--ts": "echo \"@offirmo-private/murmurhash\" && tsc --noEmit",
		"test--ts--watch": "tsc --noEmit --watch",
		"dev": "run-s test--ts--watch",
		"test": "run-s test--unit test--ts"
	},
	"devDependencies": {
		"@offirmo-private/monorepo-scripts": "*",
		"@offirmo/unit-test-toolbox": "^9",
		"@types/murmurhash3js-revisited": "^3",
		"@types/node": "^22",
		"npm-run-all": "^4",
		"tslib": "^2",
		"typescript": "^5"
	}
}
