{
	"name": "@offirmo-private/json-stable-stringify",
	"description": "A private fork of https://github.com/substack/json-stable-stringify",
	"version": "0.0.1",
	"author": "Offirmo <offirmo.net@gmail.com>",
	"license": "MIT",
	"private": true,

	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/src.es2024.esm/index.js"
		}
	},
	"module": "dist/src.es2024.esm/index.js",
	"typings": "dist/src.es2024.esm/index.d.ts",
	"source": "src/index.ts",

	"size-limit": [
		{
			"limit": "100b",
			"path": "dist/src.es2024.esm/index.js"
		}
	],

	"peerDependencies": {
	},
	"dependencies": {
		"@offirmo-private/ts-types": "*"
	},

	"scripts": {
		"clean": "monorepo-script--clean-package â€¦dist",

		"_build:dev:watch": "monorepo-script--build-typescript-package --watch",
		"_build:prod": "monorepo-script--build-typescript-package",
		"ensure-size": "size-limit",

		"dev": "run-s clean _build:dev:watch",
		"test": "mocha --bail --config ../../node_modules/@offirmo/unit-test-toolbox/mocharc.json --file ../../node_modules/@offirmo/unit-test-toolbox/mocha-chai-init-node.mjs 'dist/src.es2024.esm/**/*spec.js'",
		"build": "run-s  clean _build:prod"
	},
	"devDependencies": {
		"@offirmo-private/monorepo-scripts": "*",
		"@offirmo/unit-test-toolbox": "^9",
		"@size-limit/preset-small-lib": "^11",
		"npm-run-all": "^4",
		"size-limit": "^11"
	}
