{
	"name": "@monorepo-private/pkg-infos-resolver",
	"description": "Resolves infos about packages such as their version, typings, etc.",
	"version": "0.0.1",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": "./module/index.ts"
	},
	"source": "module/index.ts",
	"peerDependencies": {
		"tslib": "^2"
	},
	"dependencies": {
		"@monorepo-private/monorepo": "*",
		"package-json": "^10",
		"semver": "^7",
		"tiny-invariant": "^1"
	},
	"scripts": {
		"check--ts": "echo \"@monorepo-private/pkg-infos-resolver\" && tsc --noEmit",
		"check--ts--watch": "tsc --noEmit --watch",
		"dev": "run-s check--ts--watch",
		"check": "run-s check--ts"
	},
	"devDependencies": {
		"@monorepo-private/monorepo-scripts": "*",
		"@offirmo/unit-test-toolbox": "^9",
		"@types/node": "^24",
		"@types/semver": "^7",
		"npm-run-all": "^4",
		"tslib": "^2",
		"typescript": "^5"
	}
}
