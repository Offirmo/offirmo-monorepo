{
	"name": "@monorepo-private/pure-module--analyzer",
	"description": "Walk/parse a pure module and extract infos from it in order to generate build-related files",
	"version": "0.0.1",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": "./module/src/index.ts"
	},
	"source": "module/src/index.ts",
	"peerDependencies": {
		"tslib": "^2"
	},
	"dependencies": {
		"@monorepo-private/file-entry": "*",
		"@monorepo-private/pkg-infos-resolver": "*",
		"ignore-walk": "^8",
		"json5": "^2",
		"parse-imports-ts": "^1",
		"tiny-invariant": "^1",
		"write-json-file": "^7"
	},
	"scripts": {
		"check--ts": "echo \"@monorepo-private/pure-module--analyzer\" && tsc --noEmit",
		"check--ts--watch": "tsc --noEmit --watch",
		"dev": "run-s check--ts--watch",
		"check": "run-s check--ts",
		"demo": "node --experimental-strip-types ./module/##demo/index.ts"
	},
	"devDependencies": {
		"@monorepo-private/monorepo-scripts": "*",
		"@offirmo/unit-test-toolbox": "^9",
		"@types/ignore-walk": "^4",
		"@types/node": "^24",
		"npm-run-all": "^4",
		"tslib": "^2",
		"typescript": "^5"
	}
}
