{
	"name": "@offirmo-private/prettify-any",
	"description": "Intelligently pretty-print a JS object (and by extension, JSON), on node or browser",
	"version": "0.0.1",
	"author": "Offirmo <offirmo.net@gmail.com>",
	"license": "Unlicense",
	"private": true,

	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/src.es2022.esm/index.js",
			"require": "./dist/src.es2022.cjs/index.js"
		}
	},
	"module": "dist/src.es2022.esm/index.js",
	"main": "dist/src.es2022.cjs/index.js",
	"typings": "dist/src.es2022.esm/index.d.ts",
	"source": "src/index.ts",

	"peerDependencies": {
		"tslib": "^2"
	},
	"dependencies": {
		"@offirmo-private/ts-types": "^0",
		"indent-string": "^5",
		"terminal-size": "^4",
		"tiny-invariant": "^1"
	},

	"scripts": {
		"clean": "monorepo-script--clean-package â€¦dist",

		"_build:dev:watch": "monorepo-script--build-typescript-package --watch",
		"_build:prod": "monorepo-script--build-typescript-package",

		"dev": "run-s clean _build:dev:watch",
		"test": "mocha --bail --config ./node_modules/@offirmo/unit-test-toolbox/mocharc.json ./node_modules/@offirmo/unit-test-toolbox/mocha-chai-init-node.cjs 'dist/src.es2022.esm/**/*spec.js'",
		"build": "run-s  clean _build:prod"
	},
	"devDependencies": {
		"@offirmo-private/monorepo-scripts": "*",
		"@offirmo/unit-test-toolbox": "^8",
		"@types/prettyjson": "^0",
		"chalk": "^5",
		"npm-run-all": "^4",
		"prettyjson": "^1",
		"tslib": "^2"
	}
}
