{
	"name": "@offirmo/universal-debug-api-placeholder",
	"description": "Implementation of Offirmo’s Universal Debug Api doing nothing at all, yielding to any other effective implementation",
	"version": "1.0.1",
	"author": "Offirmo <offirmo.net@gmail.com>",
	"license": "Unlicense",

	"sideEffects": true,
	"exports": {
		".": {
			"import": "./dist/src.es2022/index.js",
			"require": "./dist/src.es2022.cjs/index.js"
		}
	},
	"module": "dist/src.es2022/index.js",
	"main": "dist/src.es2022.cjs/index.js",
	"typings": "dist/src.es2022.cjs/index.d.ts",
	"source": "src/index.ts",

	"size-limit": [
		{
			"limit": "600b",
			"path": "dist/src.es2022/index.js"
		},
		{
			"limit": "700b",
			"path": "dist/src.es2022.cjs/index.js"
		},
		{
			"limit": "1.3kb",
			"path": "dist/index-bundle.js"
		}
	],

	"peerDependencies": {
	},
	"dependencies": {
		"@offirmo/globalthis-ponyfill": "^4",
		"@offirmo/practical-logger-minimal-noop": "^3",
		"@offirmo/universal-debug-api-interface": "^1"
	},

	"devDependencies": {
		"@offirmo-private/monorepo-scripts": "*",
		"@offirmo/practical-logger-core": "^3",
		"@size-limit/preset-small-lib": "^7",
		"npm-run-all": "^4",
		"size-limit": "^7",
		"ts-node": "^10"
	},
	"scripts": {
		"clean": "monorepo-script--clean-package …dist",

		"_build:dev:watch": "monorepo-script--build-typescript-package --watch",
		"_build:prod": "monorepo-script--build-typescript-package",
		"ensure-size": "size-limit",

		"dev": "run-s clean _build:dev:watch",
		"build": "run-p _build:prod",
		"cheatsheet": "node doc/cheatsheet.js",
		"demo:node": "./doc/demo.ts",
		"demo": "parcel --out-dir .parcel --no-autoinstall doc/demo-web/index.html",

		"np": "np --no-publish",
		"prepublishOnly": "run-s clean build ensure-size"
	},

	"files": [
		"dist",
		"src"
	],
	"repository": "https://github.com/Offirmo/offirmo-monorepo--2022/tree/main/stack--2022/3-advanced--multi/universal-debug-api-placeholder",
	"keywords": [
		"debug",
		"logger",
		"typescript"
	],
	"homepage": "https://github.com/Offirmo/offirmo-monorepo--2022/tree/main/stack--2022/3-advanced--multi/universal-debug-api-placeholder",
	"bugs": {
		"url": "https://github.com/Offirmo/offirmo-monorepo/issues"
	}
}
